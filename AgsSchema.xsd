<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
  <xs:element name="AGSEditorDocument">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Game"/>
        <xs:element maxOccurs="unbounded" ref="Component"/>
      </xs:sequence>
      <xs:attribute name="EditorVersion" use="required" type="xs:NMTOKEN"/>
      <xs:attribute name="Version" use="required" type="xs:NMTOKEN"/>
      <xs:attribute name="VersionIndex" use="required" type="xs:integer"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Game">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Settings"/>
        <xs:element ref="RuntimeSetup"/>
        <xs:element ref="LipSync"/>
        <xs:element ref="PropertyDefinitions"/>
        <xs:element ref="GlobalMessages"/>
        <xs:element ref="OldInteractionVariables"/>
        <xs:element ref="Plugins"/>
        <xs:element ref="Rooms"/>
        <xs:element ref="GUIs"/>
        <xs:element ref="InventoryItems"/>
        <xs:element ref="TextParser"/>
        <xs:element ref="Characters"/>
        <xs:element ref="PlayerCharacter"/>
        <xs:element ref="Dialogs"/>
        <xs:element ref="Cursors"/>
        <xs:element ref="Fonts"/>
        <xs:element ref="Palette"/>
        <xs:element ref="GlobalVariables"/>
        <xs:element ref="Sprites"/>
        <xs:element ref="Views"/>
        <xs:element ref="DeletedViews"/>
        <xs:element ref="Scripts"/>
        <xs:element ref="AudioClips"/>
        <xs:element ref="AudioClipTypes"/>
        <xs:element ref="Translations"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="RuntimeSetup">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="GraphicsDriver"/>
        <xs:element ref="Windowed"/>
        <xs:element ref="FullscreenGameScaling"/>
        <xs:element ref="GameScaling"/>
        <xs:element ref="GameScalingMultiplier"/>
        <xs:element ref="GraphicsFilter"/>
        <xs:element ref="VSync"/>
        <xs:element ref="AAScaledSprites"/>
        <xs:element ref="DowngradeTo16bit"/>
        <xs:element ref="RenderAtScreenResolution"/>
        <xs:element ref="DigitalSound"/>
        <xs:element ref="MidiSound"/>
        <xs:element ref="UseVoicePack"/>
        <xs:element ref="ThreadedAudio"/>
        <xs:element ref="Translation"/>
        <xs:element ref="AutoLockMouse"/>
        <xs:element ref="MouseSpeed"/>
        <xs:element ref="SpriteCacheSize"/>
        <xs:element ref="UseCustomSavePath"/>
        <xs:element ref="CustomSavePath"/>
        <xs:element ref="UseCustomAppDataPath"/>
        <xs:element ref="CustomAppDataPath"/>
        <xs:element ref="TitleText"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="GraphicsDriver" type="xs:NCName"/>
  <xs:element name="Windowed" type="xs:NCName"/>
  <xs:element name="FullscreenGameScaling" type="xs:NCName"/>
  <xs:element name="GameScaling" type="xs:NCName"/>
  <xs:element name="GameScalingMultiplier" type="xs:integer"/>
  <xs:element name="GraphicsFilter" type="xs:NCName"/>
  <xs:element name="VSync" type="xs:NCName"/>
  <xs:element name="AAScaledSprites" type="xs:NCName"/>
  <xs:element name="DowngradeTo16bit" type="xs:NCName"/>
  <xs:element name="DigitalSound" type="xs:NCName"/>
  <xs:element name="MidiSound" type="xs:NCName"/>
  <xs:element name="UseVoicePack" type="xs:NCName"/>
  <xs:element name="ThreadedAudio" type="xs:NCName"/>
  <xs:element name="Translation">
    <xs:complexType/>
  </xs:element>
  <xs:element name="AutoLockMouse" type="xs:NCName"/>
  <xs:element name="MouseSpeed" type="xs:integer"/>
  <xs:element name="SpriteCacheSize" type="xs:integer"/>
  <xs:element name="UseCustomSavePath" type="xs:NCName"/>
  <xs:element name="CustomSavePath">
    <xs:complexType/>
  </xs:element>
  <xs:element name="UseCustomAppDataPath" type="xs:NCName"/>
  <xs:element name="CustomAppDataPath">
    <xs:complexType>
      <xs:attribute name="IsNull" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="TitleText">
    <xs:complexType mixed="true">
      <xs:attribute ref="xml:space" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="LipSync">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Type"/>
        <xs:element ref="DefaultFrame"/>
        <xs:element ref="Frames"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="DefaultFrame" type="xs:integer"/>
  <xs:element name="PropertyDefinitions">
    <xs:complexType/>
  </xs:element>
  <xs:element name="GlobalMessages">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="Message"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Message">
    <xs:complexType>
      <xs:attribute name="ID" use="required" type="xs:integer"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="OldInteractionVariables">
    <xs:complexType/>
  </xs:element>
  <xs:element name="Plugins">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Plugin"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Plugin">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="FileName"/>
        <xs:element ref="Data"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Data">
    <xs:complexType/>
  </xs:element>
  <xs:element name="Rooms">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="UnloadedRoomFolder"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="UnloadedRoomFolder">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="SubFolders"/>
        <xs:element ref="UnloadedRooms"/>
      </xs:sequence>
      <xs:attribute name="Name" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="UnloadedRooms">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="UnloadedRoom"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="UnloadedRoom">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Number"/>
        <xs:element ref="Description"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Number" type="xs:integer"/>
  <xs:element name="TextParser">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Words"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Words">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="TextParserWord"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TextParserWord">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="WordGroup"/>
        <xs:element ref="Word"/>
        <xs:element ref="Type"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="WordGroup" type="xs:integer"/>
  <xs:element name="Word" type="xs:NCName"/>
  <xs:element name="PlayerCharacter" type="xs:integer"/>
  <xs:element name="Cursors">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="MouseCursor"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="MouseCursor">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ID"/>
        <xs:element ref="HotspotX"/>
        <xs:element ref="HotspotY"/>
        <xs:element ref="StandardMode"/>
        <xs:element ref="Image"/>
        <xs:element ref="View"/>
        <xs:element ref="Animate"/>
        <xs:element ref="AnimateOnlyOnHotspots"/>
        <xs:element ref="AnimateOnlyWhenMoving"/>
        <xs:element ref="Name"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="StandardMode" type="xs:NCName"/>
  <xs:element name="Animate" type="xs:NCName"/>
  <xs:element name="AnimateOnlyOnHotspots" type="xs:NCName"/>
  <xs:element name="AnimateOnlyWhenMoving" type="xs:NCName"/>
  <xs:element name="Fonts">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="Font"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Palette">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="PaletteEntry"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="PaletteEntry">
    <xs:complexType>
      <xs:attribute name="Blue" use="required" type="xs:integer"/>
      <xs:attribute name="Green" use="required" type="xs:integer"/>
      <xs:attribute name="Index" use="required" type="xs:integer"/>
      <xs:attribute name="Red" use="required" type="xs:integer"/>
      <xs:attribute name="Type" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="GlobalVariables">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Variables"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Variables">
    <xs:complexType/>
  </xs:element>
  <xs:element name="DeletedViews">
    <xs:complexType/>
  </xs:element>
  <xs:element name="Scripts">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ScriptFolder"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ScriptFolder">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="SubFolders"/>
        <xs:element ref="ScriptAndHeaders"/>
      </xs:sequence>
      <xs:attribute name="Name" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ScriptAndHeaders">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="ScriptAndHeader"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ScriptAndHeader">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ScriptAndHeader_Header"/>
        <xs:element ref="ScriptAndHeader_Script"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ScriptAndHeader_Header" type="Script"/>
  <xs:element name="ScriptAndHeader_Script" type="Script"/>
  <xs:element name="AudioClipTypes">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="AudioClipType"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="AudioClipType">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="TypeID"/>
        <xs:element ref="Name"/>
        <xs:element ref="MaxChannels"/>
        <xs:element ref="VolumeReductionWhileSpeechPlaying"/>
        <xs:element ref="CrossfadeClips"/>
        <xs:element ref="BackwardsCompatibilityType"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TypeID" type="xs:integer"/>
  <xs:element name="MaxChannels" type="xs:integer"/>
  <xs:element name="VolumeReductionWhileSpeechPlaying" type="xs:integer"/>
  <xs:element name="CrossfadeClips" type="xs:NCName"/>
  <xs:element name="BackwardsCompatibilityType" type="xs:NCName"/>
  <xs:element name="Component">
    <xs:complexType>
      <xs:choice minOccurs="0">
        <xs:element ref="ProjectUnderControl"/>
        <xs:element ref="SpeechCenter"/>
        <xs:sequence>
          <xs:element ref="SpeechVoxFiles"/>
          <xs:element ref="PamFiles"/>
        </xs:sequence>
      </xs:choice>
      <xs:attribute name="Name" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ProjectUnderControl" type="xs:NCName"/>
  <xs:element name="SpeechCenter">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Document"/>
      </xs:sequence>
      <xs:attribute name="Version" use="required" type="xs:NMTOKEN"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Document">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Settings"/>
        <xs:element ref="SpeechLines"/>
        <xs:element ref="Translations"/>
      </xs:sequence>
      <xs:attribute name="Version" use="required" type="xs:NMTOKEN"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="SpeechLines">
    <xs:complexType/>
  </xs:element>
  <xs:element name="SpeechVoxFiles">
    <xs:complexType/>
  </xs:element>
  <xs:element name="PamFiles">
    <xs:complexType/>
  </xs:element>
  <xs:element name="Settings">
    <xs:complexType>
      <xs:choice>
        <xs:sequence>
          <xs:element ref="CustomFunctions"/>
          <xs:element ref="VoiceActorScripts"/>
          <xs:element ref="Translations"/>
          <xs:element ref="Edit"/>
          <xs:element ref="DialogParsing"/>
        </xs:sequence>
        <xs:sequence>
          <xs:element ref="GameFileName"/>
          <xs:element ref="GameName"/>
          <xs:element ref="UniqueID"/>
          <xs:element ref="GUIDAsString"/>
          <xs:element ref="ColorDepth"/>
          <xs:element ref="CustomResolution"/>
          <xs:element ref="AllowRelativeAssetResolutions"/>
          <xs:element ref="CompressSprites"/>
          <xs:element ref="SaveScreenshots"/>
          <xs:element ref="RoomTransition"/>
          <xs:element ref="MouseWheelEnabled"/>
          <xs:element ref="UseLowResCoordinatesInScript"/>
          <xs:element ref="TurnBeforeFacing"/>
          <xs:element ref="TurnBeforeWalking"/>
          <xs:element ref="ScaleMovementSpeedWithMaskResolution"/>
          <xs:element ref="SplitResources"/>
          <xs:element ref="LetterboxMode"/>
          <xs:element ref="AutoMoveInWalkMode"/>
          <xs:element ref="PixelPerfect"/>
          <xs:element ref="WhenInterfaceDisabled"/>
          <xs:element ref="GUIAlphaStyle"/>
          <xs:element ref="SpriteAlphaStyle"/>
          <xs:element ref="RunGameLoopsWhileDialogOptionsDisplayed"/>
          <xs:element ref="InventoryHotspotMarkerStyle"/>
          <xs:element ref="InventoryHotspotMarkerSprite"/>
          <xs:element ref="InventoryHotspotMarkerDotColor"/>
          <xs:element ref="InventoryHotspotMarkerCrosshairColor"/>
          <xs:element ref="WalkInLookMode"/>
          <xs:element ref="AntiGlideMode"/>
          <xs:element ref="DebugMode"/>
          <xs:element ref="InventoryCursors"/>
          <xs:element ref="HandleInvClicksInScript"/>
          <xs:element ref="DisplayMultipleInventory"/>
          <xs:element ref="ScriptAPIVersion"/>
          <xs:element ref="ScriptCompatLevel"/>
          <xs:element ref="EnforceObjectBasedScript"/>
          <xs:element ref="EnforceNewStrings"/>
          <xs:element ref="EnforceNewAudio"/>
          <xs:element ref="UseOldCustomDialogOptionsAPI"/>
          <xs:element ref="LeftToRightPrecedence"/>
          <xs:element ref="PlaySoundOnScore"/>
          <xs:element ref="CrossfadeMusic"/>
          <xs:element ref="DialogOptionsGUI"/>
          <xs:element ref="DialogOptionsGap"/>
          <xs:element ref="DialogOptionsBullet"/>
          <xs:element ref="SkipSpeech"/>
          <xs:element ref="SpeechStyle"/>
          <xs:element ref="UseGlobalSpeechAnimationDelay"/>
          <xs:element ref="GlobalSpeechAnimationDelay"/>
          <xs:element ref="LegacySpeechAnimationSpeed"/>
          <xs:element ref="NumberDialogOptions"/>
          <xs:element ref="DialogOptionsBackwards"/>
          <xs:element ref="SpeechPortraitSide"/>
          <xs:element ref="DialogScriptSayFunction"/>
          <xs:element ref="DialogScriptNarrateFunction"/>
          <xs:element ref="TextWindowGUI"/>
          <xs:element ref="AlwaysDisplayTextAsSpeech"/>
          <xs:element ref="AntiAliasFonts"/>
          <xs:element ref="ThoughtGUI"/>
          <xs:element ref="BackwardsText"/>
          <xs:element ref="MaximumScore"/>
          <xs:element ref="HasMODMusic"/>
          <xs:element ref="DefaultRoomMaskResolution"/>
          <xs:element ref="GameExplorerEnabled"/>
          <xs:element ref="Description"/>
          <xs:element ref="ReleaseDate"/>
          <xs:element ref="Genre"/>
          <xs:element ref="Version"/>
          <xs:element ref="WindowsExperienceIndex"/>
          <xs:element ref="DeveloperName"/>
          <xs:element ref="DeveloperURL"/>
          <xs:element ref="EnhancedSaveGames"/>
          <xs:element ref="SaveGameFileExtension"/>
          <xs:element ref="SaveGameFolderName"/>
          <xs:element ref="BinaryFilesInSourceControl"/>
          <xs:element ref="AudioIndexer"/>
          <xs:element ref="BuildTargets"/>
          <xs:element ref="RenderAtScreenResolution"/>
        </xs:sequence>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="CustomFunctions">
    <xs:complexType/>
  </xs:element>
  <xs:element name="VoiceActorScripts">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Format"/>
      </xs:sequence>
      <xs:attribute name="NewParagraph" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Format">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Styles"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Styles">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="CSSStyle"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CSSStyle">
    <xs:complexType>
      <xs:attribute name="CSSName" use="required" type="xs:NMTOKEN"/>
      <xs:attribute name="Color" use="required"/>
      <xs:attribute name="FontFamily" use="required"/>
      <xs:attribute name="FontStyle" use="required" type="xs:NCName"/>
      <xs:attribute name="FontWeight" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Edit">
    <xs:complexType>
      <xs:attribute name="ExitEditModeAfterSave" use="required" type="xs:boolean"/>
      <xs:attribute name="RefreshProjectAfterSave" use="required" type="xs:NCName"/>
      <xs:attribute name="ShowWarning" use="required" type="xs:boolean"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="DialogParsing">
    <xs:complexType>
      <xs:attribute name="ShowAllOoptionText" use="required" type="xs:boolean"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="GameFileName" type="xs:NCName"/>
  <xs:element name="GameName" type="xs:NCName"/>
  <xs:element name="UniqueID" type="xs:integer"/>
  <xs:element name="GUIDAsString" type="xs:string"/>
  <xs:element name="ColorDepth" type="xs:NCName"/>
  <xs:element name="CustomResolution" type="xs:string"/>
  <xs:element name="AllowRelativeAssetResolutions" type="xs:NCName"/>
  <xs:element name="CompressSprites" type="xs:NCName"/>
  <xs:element name="SaveScreenshots" type="xs:NCName"/>
  <xs:element name="RoomTransition" type="xs:NCName"/>
  <xs:element name="MouseWheelEnabled" type="xs:NCName"/>
  <xs:element name="UseLowResCoordinatesInScript" type="xs:NCName"/>
  <xs:element name="TurnBeforeFacing" type="xs:NCName"/>
  <xs:element name="ScaleMovementSpeedWithMaskResolution" type="xs:NCName"/>
  <xs:element name="SplitResources" type="xs:integer"/>
  <xs:element name="LetterboxMode" type="xs:NCName"/>
  <xs:element name="AutoMoveInWalkMode" type="xs:NCName"/>
  <xs:element name="PixelPerfect" type="xs:NCName"/>
  <xs:element name="WhenInterfaceDisabled" type="xs:NCName"/>
  <xs:element name="GUIAlphaStyle" type="xs:NCName"/>
  <xs:element name="SpriteAlphaStyle" type="xs:NCName"/>
  <xs:element name="RunGameLoopsWhileDialogOptionsDisplayed" type="xs:NCName"/>
  <xs:element name="InventoryHotspotMarkerStyle" type="xs:NCName"/>
  <xs:element name="InventoryHotspotMarkerSprite" type="xs:integer"/>
  <xs:element name="InventoryHotspotMarkerDotColor" type="xs:integer"/>
  <xs:element name="InventoryHotspotMarkerCrosshairColor" type="xs:integer"/>
  <xs:element name="WalkInLookMode" type="xs:NCName"/>
  <xs:element name="AntiGlideMode" type="xs:NCName"/>
  <xs:element name="DebugMode" type="xs:NCName"/>
  <xs:element name="InventoryCursors" type="xs:NCName"/>
  <xs:element name="HandleInvClicksInScript" type="xs:NCName"/>
  <xs:element name="DisplayMultipleInventory" type="xs:NCName"/>
  <xs:element name="ScriptAPIVersion" type="xs:NCName"/>
  <xs:element name="ScriptCompatLevel" type="xs:NCName"/>
  <xs:element name="EnforceObjectBasedScript" type="xs:NCName"/>
  <xs:element name="EnforceNewStrings" type="xs:NCName"/>
  <xs:element name="EnforceNewAudio" type="xs:NCName"/>
  <xs:element name="UseOldCustomDialogOptionsAPI" type="xs:NCName"/>
  <xs:element name="LeftToRightPrecedence" type="xs:NCName"/>
  <xs:element name="PlaySoundOnScore" type="xs:integer"/>
  <xs:element name="CrossfadeMusic" type="xs:NCName"/>
  <xs:element name="DialogOptionsGUI" type="xs:integer"/>
  <xs:element name="DialogOptionsGap" type="xs:integer"/>
  <xs:element name="DialogOptionsBullet" type="xs:integer"/>
  <xs:element name="SkipSpeech" type="xs:NCName"/>
  <xs:element name="SpeechStyle" type="xs:NCName"/>
  <xs:element name="UseGlobalSpeechAnimationDelay" type="xs:NCName"/>
  <xs:element name="GlobalSpeechAnimationDelay" type="xs:integer"/>
  <xs:element name="LegacySpeechAnimationSpeed" type="xs:NCName"/>
  <xs:element name="NumberDialogOptions" type="xs:NCName"/>
  <xs:element name="DialogOptionsBackwards" type="xs:NCName"/>
  <xs:element name="SpeechPortraitSide" type="xs:NCName"/>
  <xs:element name="DialogScriptSayFunction">
    <xs:complexType>
      <xs:attribute name="IsNull" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="DialogScriptNarrateFunction">
    <xs:complexType>
      <xs:attribute name="IsNull" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="AlwaysDisplayTextAsSpeech" type="xs:NCName"/>
  <xs:element name="AntiAliasFonts" type="xs:NCName"/>
  <xs:element name="ThoughtGUI" type="xs:integer"/>
  <xs:element name="BackwardsText" type="xs:NCName"/>
  <xs:element name="MaximumScore" type="xs:integer"/>
  <xs:element name="HasMODMusic" type="xs:NCName"/>
  <xs:element name="DefaultRoomMaskResolution" type="xs:integer"/>
  <xs:element name="GameExplorerEnabled" type="xs:NCName"/>
  <xs:element name="ReleaseDate" type="xs:date"/>
  <xs:element name="Genre" type="xs:NCName"/>
  <xs:element name="WindowsExperienceIndex" type="xs:integer"/>
  <xs:element name="DeveloperName">
    <xs:complexType/>
  </xs:element>
  <xs:element name="DeveloperURL">
    <xs:complexType/>
  </xs:element>
  <xs:element name="EnhancedSaveGames" type="xs:NCName"/>
  <xs:element name="SaveGameFileExtension">
    <xs:complexType/>
  </xs:element>
  <xs:element name="SaveGameFolderName" type="xs:NCName"/>
  <xs:element name="BinaryFilesInSourceControl" type="xs:NCName"/>
  <xs:element name="AudioIndexer" type="xs:integer"/>
  <xs:element name="BuildTargets">
    <xs:complexType mixed="true">
      <xs:attribute ref="xml:space" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="RenderAtScreenResolution" type="xs:NCName"/>
  <xs:element name="Type" type="xs:NCName"/>
  <xs:element name="Frames">
    <xs:complexType>
      <xs:choice>
        <xs:element maxOccurs="unbounded" ref="CharsForFrame"/>
        <xs:element maxOccurs="unbounded" ref="ViewFrame"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="CharsForFrame">
    <xs:complexType/>
  </xs:element>
  <xs:element name="ViewFrame">
    <xs:complexType>
      <xs:sequence>
